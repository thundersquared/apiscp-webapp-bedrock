{
    "name": "thundersquared/apiscp-webapp-bedrock",
    "description": "A Bedrock WordPress application for ApisCP",
    "keywords": [
        "apiscp",
        "bedrock",
        "wordpress"
    ],
    "homepage": "https://github.com/thundersquared/apiscp-webapp-bedrock",
    "license": "MIT",
    "authors": [
        {
            "name": "Anatoli Nicolae",
            "email": "an@thundersquared.com"
        }
    ],
    "autoload": {
        "classmap": [
            "lib/"
        ],
        "exclude-from-classmap": []
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false
    },
    "type": "apiscp-webapp",
    "require": {
        "adbario/php-dot-notation": "^3.1",
        "sixlive/dotenv-editor": "^2.0"
    }
}
